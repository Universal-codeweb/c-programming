#include<stdio.h>
int main()
{
    int n,sum=0;
    printf("enter the size of an array:\n ");
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        printf("a[%d]=\n",i);
        scanf("%d",&a[i]);
        sum=sum+a[i];
    
    }
    printf("sum of the array=%d",sum);
    
 return 0;   
}



